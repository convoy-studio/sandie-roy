define ["Page"], (Page) ->

    class <%= className %> extends Page

        constructor: (id, scope) ->
            super(id, scope)

        init: =>
            super()

        ready: =>
            super()

        addAnimations: =>
            super()

        transitionIn: =>
            super()

        transitionOut: =>
            super()

        transitionInCompleted: =>
            super()

        transitionOutCompleted: =>
            super()

        resize: =>
            super()

        destroy: =>
            super()

    return <%= className %>

